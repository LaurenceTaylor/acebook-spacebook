<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%=form_tag sessions_path do |form| %>
    <p>
      <%=label_tag :email %> <br>
      <%=text_field_tag :email, params[:email] %> <br>
      <%=label_tag :password %> <br>
      <%=password_field_tag :password %> <br>
      <%=submit_tag "Sign In" %>
    </p>
    <% end %>

  </body>
</html>
